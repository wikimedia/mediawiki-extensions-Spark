{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery"
	],
	"globals": {
		"OO": false,
		"jQuery": false,
		"mw": false
	},
	"rules": {
		"vars-on-top": "warn",
		"no-unused-vars": "warn",
		"no-undef": "warn",
		"no-implicit-globals": "warn",
		"one-var": "warn",
		"eqeqeq": "warn",
		"camelcase": "warn",
		"no-useless-escape": "warn",
		"no-shadow": "warn",
		"no-use-before-define": "warn",
		"max-statements-per-line": "warn",
		"no-jquery/no-in-array": "warn",
		"no-jquery/no-global-selector": "warn",
		"no-jquery/no-each-util": "warn"
	}
}
